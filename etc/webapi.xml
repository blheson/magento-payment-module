<?xml version="1.0"?>
<routes xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:noNamespaceSchemaLocation="urn:magento:module:Magento_Webapi:etc/webapi.xsd">

    <route method="POST" url="/V1/rocketfuel-callback">
        <service class="Rocketfuel\Rocketfuel\Api\BackendInterface" method="postCallback"/>
        <resources>
            <resource ref="anonymous"/>
        </resources>
    </route>
    <route method="GET" url="/V1/rocketfuel-callback">
        <service class="Rocketfuel\Rocketfuel\Api\BackendInterface" method="getCallback"/>
        <resources>
            <resource ref="anonymous"/>
        </resources>
    </route>
    <route method="GET" url="/V1/rocketfuel-auth">
        <service class="Rocketfuel\Rocketfuel\Api\BackendInterface" method="getAuth"/>
        <resources>
            <resource ref="anonymous"/>
        </resources>
    </route>
    <route method="GET" url="/V1/rocketfuel-get-payload/:id">
        <service class="Rocketfuel\Rocketfuel\Api\BackendInterface" method="getRocketfuelPayload"/>
        <resources>
            <resource ref="anonymous"/>
        </resources>
    </route>
</routes>
